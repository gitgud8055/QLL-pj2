  <script>
    $("#off-d").click(function() {$("#date").hide();});
    $("#get-table button").click(function() {
      const tr_block = $(this).closest("tr").find("td");
      $("#lOiMajcY").text(tr_block.eq(0).html());
      $("#PoKauJw").text("");
      fetch(`/api/get-date?id=${tr_block.eq(0).html()}`, {method: "GET"})
      .then(async function(response) {
        if (!response.ok) {
          let x = await response.json();
          throw new Error(`Error: ${x.message}`);
        }
        return response.json();
      }).then(data => {
        data.forEach((v, i) => {
          $("#PoKauJw").append(`
          <div class="input-group my-2">
            <div class="input-group-text">
              <input type="radio" id="cNjUai${i}" name="select-prefer-date" class="form-check-input" value="${i}">
            </div>
            <label class="input-group-text" for="cNjUai${i}">${v.st} - ${v.ed} (${v.dur} phút)</label>
            <input type="datetime-local" class="form-control" name="date" min='${v.st}' max='${v.ed}' step='${v.dur * 60}' id="PaoKLhA${i}" disabled>
          </div>`);
        });
        $("#PoKauJw input[type='radio']").click(function() {
          $("#PoKauJw input[type='datetime-local']").prop('disabled', true);
          $(`#PaoKLhA${$(this).val()}`).prop('disabled', false);
        });
        $("#PoKauJw input[type='datetime-local']").change(function() {
          this.reportValidity();
        });
      }).catch(err => {alert(err.message);});

      $("#date").show();
    });

    $("#submit-date").click(function() {
      const data = new FormData( document.getElementById("form-date"));
      data.append('id', $("#lOiMajcY").text());
      fetch('/api/set-user-date', {
        method: "POST",
        body: data
      }).then(async function(response) {
        if (!response.ok) {
          let x = await response.json();
          throw new Error(`Error: ${x.message}`);
        }
        return response.json();
      }).then(data => {alert(data.message); }).catch(err => {alert(err.message);});
    });
  </script>
  <script>
    $(".off-i").click(function() {$("#info").hide();});
    $(".noted-v1").click(function() {
      $("#OiwJuma").val($(this).text());
      let cur = $(this).parent().find("td");
      $("#PlqUak").text(cur.eq(0).text());
      $("#MqaiUw").parent().show();
      $("#NajycTa").parent().show();
      $("#MqaiUw").text(cur.eq(1).text());
      $("#NajycTa").text(cur.eq(2).text());
      $("#info").show();
    });
    $(".noted-v2").click(function() {
      $("#OiwJuma").val($(this).text());
      $("#PlqUak").text($(this).parent().find("td").eq(0).text());
      $("#MqaiUw").parent().hide();
      $("#NajycTa").parent().hide();
      $("#info").show();
    });
    $("#sm").click(function() {
      const data = new FormData(document.getElementById("form-i"));
      data.append('id', $("#PlqUak").text());
      data.append('user', $("#MqaiUw").text());
      for (const [n, v] of data.entries()) {
        console.log(n, v);
      }
      fetch('/api/change-note', {method: "POST", body: data})
      .then(async function(response) {
				if (!response.ok) {
					let x = await response.json();
