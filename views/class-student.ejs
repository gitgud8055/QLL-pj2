    $("#data-table tr:not([class='skip'])").click(function() {
      $(this).toggleClass("table-primary");
      let cur = $(this).find("input[type='checkbox']");
      cur.prop('checked', $(this).hasClass("table-primary"));
    });
    $("#select-all-box").click(function() {
      console.log($(this));
      let cur = $(this).prop('checked');
      $("#data-table tr:not([class='skip'], [style*='display: none'])").each((i, v) => {
        if (cur != $(v).hasClass("table-primary")) {
          $(v).click();
        }
      });
    });
    let SelectorAllElement = false;
    $("#data-table tr:not([class='skip'])").mouseover(function(event) {
      if (SelectorAllElement) {
        $(this).click();
      }
    });
    $(document).keydown((event) => {
      if (event.key === "Shift") {
        SelectorAllElement = !SelectorAllElement;
      }
    });
  </script>
